  "scripts": {
    "postinstall": "typings install",
    "build": "gulp build:prod",
    "watch": "gulp watch",
    "serve": "lite-server -c=./config/bs-config.json",
    "prestart": "gulp build",
    "start": "concurrently \"npm run serve\" \"npm run watch\"",
    "cordova-build": "gulp build:cordova && (cd mobile && cordova build && cd..)",
    "cordova-emulate": "(cd mobile && cordova emulate android && cd..)",
    "cordova-full": "gulp build:cordova && (cd mobile && cordova build android && cordova emulate android && cd..)"
  },
